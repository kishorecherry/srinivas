 
pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/kishorecherry/srinivas.git'
            }
        }
        stage('Sending image to DockerHUB') {
            steps { 
                withCredentials([usernamePassword(credentialsId: 'pahana', passwordVariable: 'mypassword', usernameVariable: 'myusername')]) {
    sh 'sudo docker push pahanakishore/front'
}
            }
        }
    }
}
